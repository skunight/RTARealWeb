<div class="modal fade" id="createProviderMemberModal">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title">新增供应商账号</h4>
            </div>
            <form role="form" class="form-horizontal">
            <div class="modal-body">
                   <div class="form-group">
                        <label for="name" class="col-md-2 control-label">账号</label>
                        <div class="col-md-10"><input type="text" class="form-control" placeholder="请输入手机号" id="name"></div>
                   </div>
                   <div class="form-group">
                       <label for="contactName" class="col-md-2 control-label">姓名</label>
                       <div class="col-md-10"><input type="text" class="form-control" placeholder="请输入姓名" id="contactName"></div>
                   </div>
                   <div class="form-group">
                       <label for="contactPhone" class="col-md-2 control-label">邮箱</label>
                       <div class="col-md-10"><input type="text" class="form-control" placeholder="请输入邮箱" id="contactPhone"></div>
                   </div>
                   <div class="form-group">
                       <label for="proCode" class="col-md-2 control-label">联系电话</label>
                       <div class="col-md-10"><input type="text" class="form-control" placeholder="请输入联系电话" id="proCode"></div>
                   </div>
                   <div class="form-group">
                       <label for="balanceType" class="col-md-2 control-label">性别</label>
                       <div class="col-md-10"><input type="text" class="form-control" placeholder="请输入性别" id="balanceType"></div>
                   </div>
                   <div class="form-group">
                       <label for="returnType" class="col-md-2 control-label">身份证</label>
                       <div class="col-md-10"><input type="text" class="form-control" placeholder="请输入身份证号码" id="returnType"></div>
                   </div>
                   <div class="form-group">
                       <label for="providerInfo" class="col-md-2 control-label">所属供应商</label>
                       <div class="col-md-10">
                            <select class="form-control" id="providerInfo">
                                <option value="providerID">providerName</option>
                            </select>
                       </div>
                   </div>
                   <div class="form-group">
                    <label for="passwd" class="col-md-2 control-label">初始密码</label>
                    <div class="col-md-10"><input type="password" class="form-control" placeholder="请输入账号密码" id="passwd"></div>
                   </div>
                   <div class="form-group">
                      <label for="operatorName" class="col-md-2 control-label">操作员</label>
                      <div class="col-md-10"><input type="password" value="" class="form-control" id="operatorName" readonly></div>
                   </div>
                   <div class="form-group">
                       <label for="isEnable" class="col-md-2 control-labdffel">是否启用</label>
                       <div class="col-md-10">
                            <div class="switch switch-small">
                                <input id="isEnable" type="checkbox" checked />
                            </div>
                       </div>
                   </div>

            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">关闭</button>
                <button type="button" class="btn btn-primary" id="createProvider">保存</button>
            </div>
            </form>
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div><!-- /.modal -->




