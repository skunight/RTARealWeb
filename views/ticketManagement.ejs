<div class="modal fade" id="createTicketModal">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title" id="modal-title">景点门票产品</h4>
            </div>
            <form role="form" class="form-horizontal">
                <div class="modal-body">
                    <div class="form-group">
                        <label for="name" class="col-md-2 control-label">产品名称</label>
                        <div class="col-md-10"><input type="text" class="form-control" placeholder="请输入产品名称" id="name"></div>
                    </div>
                    <div class="form-group">
                        <label for="subType" class="col-md-2 control-label">产品子类</label>
                        <div class="col-md-10">
                            <select class="form-control" id="subType">
                                <option value="1">
                                    电子票
                                </option>
                                <option value="2">
                                    实体票
                                </option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="content" class="col-md-2 control-label">包含内容</label>
                        <div class="col-md-10"><textarea row=3 class="form-control" placeholder="请输入产品所包含的内容" id="content"></textarea></div>
                    </div>
                    <div class="form-group">
                        <label for="intro" class="col-md-2 control-label">产品简介</label>
                        <div class="col-md-10">
                            <textarea class="form-control" id="intro" rows="3" placeholder="请输入产品简介"></textarea>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="image" class="col-md-2 control-label">图片</label>
                        <span class="col-md-2 btn btn-success fileinput-button">
                            <i class="glyphicon glyphicon-plus"></i>
                            <span>Select files...</span>
                    <!-- The file input field used as target for the file upload widget -->
                            <input type="file" multiple="" name="files[]" id="fileupload">
                        </span>
                        <label for="image" class="col-md-2 control-label">图片名称</label>
                        <div class="col-md-3"><input type="text" class="form-control" id="imgintro"></div>
                    </div>
                    <div class="form-group">
                        <label for="city" class="col-md-2 control-label">所属城市</label>
                        <div class="col-md-4">
                            <select class="form-control"  id="city">
                                <option value="shanghai">上海</option>
                            </select>
                        </div>
                        <label for="level" class="col-md-2 control-label">产品级别</label>
                        <div class="col-md-4"><input type="text" class="form-control" placeholder="请输入产品级别" id="level"></div>
                    </div>
                    <div class="form-group">
                        <label for="openTime" class="col-md-2 control-label">开园时间</label>
                        <div class="col-md-4"><input type="text" class="form-control" placeholder="请输入开园时间" id="openTime"></div>
                    </div>
                    <div class="form-group">
                        <label for="addr" class="col-md-2 control-label">地址</label>
                        <div class="col-md-10"><input type="text" class="form-control" placeholder="请输入详细地址" id="addr"></div>
                    </div>
                    <div class="form-group">
                        <label for="lat" class="col-md-2 control-label">经度</label>
                        <div class="col-md-4"><input type="text" class="form-control" placeholder="请输入地址经度" id="lat"></div>
                        <label for="lon" class="col-md-2 control-label">纬度</label>
                        <div class="col-md-4"><input type="text" class="form-control" placeholder="请输入地址纬度" id="lon"></div>
                    </div>
                    <div class="form-group">
                        <label for="bookRule" class="col-md-2 control-label">兑换规则</label>
                        <div class="col-md-10"><textarea row=3 class="form-control" placeholder="请输入兑换规则" id="bookRule"></textarea></div>
                    </div>
                    <div class="form-group">
                        <label for="useRule" class="col-md-2 control-label">使用规则</label>
                        <div class="col-md-10"><textarea row=3 class="form-control" placeholder="请输入使用规则" id="useRule"></textarea></div>
                    </div>
                    <div class="form-group">
                        <label for="cancelRule" class="col-md-2 control-label">取消规则</label>
                        <div class="col-md-10"><textarea row=3 class="form-control" placeholder="请输入取消规则" id="cancelRule"></textarea></div>
                    </div>
                    <div class="form-group">
                        <label for="transportation" class="col-md-2 control-label">交通指南</label>
                        <div class="col-md-10"><textarea row=3 class="form-control" placeholder="请输入交通指南" id="transportation"></textarea></div>
                    </div>
                    <div class="form-group">
                        <label for="effectDate" class="col-md-2 control-label">有效期</label>
                        <div class="col-md-4"><input type="text" class="form-control" placeholder="有效期开始" id="effectDate"></div>
                        <div class="col-md-1">~~</div>
                        <div class="col-md-4"><input type="text" class="form-control" placeholder="有效期结束" id="expiryDate"></div>
                    </div>
                    <div class="form-group">
                        <label for="providerInfo" class="col-md-2 control-label">所属供应商</label>
                        <div class="col-md-10">
                            <select class="form-control" id="providerInfo">
                                <option value="providerID">providerName</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="contactName" class="col-md-2 control-label">联系人</label>
                        <div class="col-md-10"><input type="text" class="form-control" placeholder="请输入产品联系人" id="contactName"></div>
                    </div>
                    <div class="form-group">
                        <label for="tel" class="col-md-2 control-label">电话</label>
                        <div class="col-md-4"><input type="text" class="form-control" placeholder="请输入联系人电话" id="tel"></div>
                        <label for="fax" class="col-md-2 control-label">传真</label>
                        <div class="col-md-4"><input type="text" class="form-control" placeholder="请输入联系人传真" id="fax"></div>
                    </div>
                    <div class="form-group">
                        <label for="operatorName" class="col-md-2 control-label">操作员</label>
                        <div class="col-md-10"><input type="text" class="form-control" id="operatorName" readonly></div>
                    </div>
                    <div class="form-group">
                        <label for="isEnable" class="col-md-2 control-label">是否启用</label>
                        <div class="col-md-10">
                            <div class="switch switch-small">
                                <input id="isEnable" type="checkbox" checked />
                            </div>
                        </div>
                    </div>
                </div>
            </form>

            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">关闭</button>
                <button type="button" class="btn btn-primary" id="createProvider">保存</button>
            </div>

        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div><!-- /.modal -->




<script src="./js/jquery.min.js"></script>
<script src="./js/bootstrap.min.js"></script>
